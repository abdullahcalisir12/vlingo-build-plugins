{
    "package": {
        "name": "vlingo-build-plugins",
        "repo": "vlingo-platform-java",
        "subject": "vlingo",
        "desc": "Build plugin supporting the vlingo platform.",
        "website_url": "https://github.com/vlingo/vlingo-build-plugins",
        "issue_tracker_url": "https://github.com/vlingo/vlingo-build-plugins/issues",
        "vcs_url": "https://github.com/vlingo/vlingo-build-plugins.git",
        "licenses": ["MPL-2.0"],
        "public_download_numbers": true,
        "public_stats": true
    },

    "version": {
        "name": "1.2.4",
        "gpgSign": true
    },

    "files": [
        {
            "includePattern": "target/(.*).jar",
            "uploadPattern": "/io/vlingo/vlingo-build-plugins/1.2.4/$1.jar",
            "matrixParams": {
                "override": 1
            }
        },
        {
            "includePattern": "pom.xml",
            "uploadPattern": "/io/vlingo/vlingo-build-plugins/1.2.4/vlingo-build-plugins-1.2.4.pom",
            "matrixParams": {
                "override": 1
            }
        }
    ],

    "publish": true
}
